export PATH=/usr/include:$PATH

# Modify bash_profile
alias edbp="vim ~/.bash_profile" # edbp = edit bash_profile
alias sbp="source ~/.bash_profile" # sbp = source bash_profile

# git alias
alias gip="git pull"
alias gad="git add"
alias gic="git commit -m"
alias gis="git status"
alias gich="git checkout"
alias gipom="git push origin master"
alias gil="git log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
